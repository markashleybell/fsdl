<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net462</TargetFramework>
        <Authors>Mark Ashley Bell</Authors>
        <Description>A code generation library for creating Dapper-based persistence layer classes with backing SQL table definitions.</Description>
        <Copyright>© Mark Ashley Bell 2018</Copyright>
        <PackageProjectUrl></PackageProjectUrl>
        <RepositoryUrl>https://github.com/markashleybell/fsdl</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <Compile Include="SqlTypes.fs" />
        <Compile Include="CSharpGenerator.fs" />
        <Compile Include="SqlGenerator.fs" />
        <Compile Include="FSDL.fs" />
        <None Include="Script.fsx" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="FSharp.Data" Version="2.4.6" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Update="System.ValueTuple" Version="4.5.0" />
    </ItemGroup>

</Project>
